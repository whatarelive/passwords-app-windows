datasource db {
    provider = "sqlite"
    url = "file:./dev.db"
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id              String @id @unique
    name            String @unique
    password        String
    webAccounts     WebsAccounts[]
}

model WebsAccounts {
    id             String @id @unique
    webName        String
    webUser        String
    webPassword    String
    webUrl         String
    user           User   @relation(fields: [userId], references: [id])
    userId         String
}